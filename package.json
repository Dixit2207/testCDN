{
    "name": "aa-ct-homepage-client",
    "version": "0.0.1",
    "description": "AA Homepage micro components",
    "scripts": {
        "assemble": "npm run build && npm run test && npm run lint",
        "build": "npm run build:prod",
        "build:qa": "ng build --project=components -c=qa --output-hashing=none --single-bundle --extra-webpack-config projects/components/webpack.partial.js",
        "postbuild:qa": "node build-components.js",
        "build:prod": "ng build --project=components -c=production --output-hashing=none --single-bundle --extra-webpack-config projects/components/webpack.partial.js",
        "postbuild:prod": "node build-components.js",
        "build:components:externals": "ng build --output-hashing=none --extra-webpack-config projects/components/webpack.externals.js --project components --single-bundle",
        "build:homepage-client": "ng build --project=homepage-client",
        "e2e": "ng e2e --project=components",
        "eslint": "npm run lint:eslint",
        "lint:all": "npm run lint:eslint && npm run lint:stylelint",
        "lint": "npm run lint:eslint && npm run lint:stylelint",
        "lint:eslint": "ng lint -- --format friendly",
        "lint:eslint:friendly": "eslint './**/*.ts' --format friendly --quiet",
        "lint:eslint:fix": "eslint './**/*.ts' --fix --format friendly --quiet",
        "lint:homepage-client": "ng lint --project=homepage-client",
        "lint:stylelint": "cross-env NODE_ENV=development stylelint './**/*.scss' --custom-formatter=./devops/stylelint/formatter",
        "ng": "ng",
        "tsc": "tsc",
        "prettier": "prettier",
        "postinstall": "ngcc -p main",
        "release": "npm run release:prod",
        "release:prod": "npm run ng deploy -c=production",
        "release:qa": "npm run ng deploy",
        "start": "ng serve",
        "start:all": "run-p start start:cdn start:bs start:homepage-client",
        "start:bs": "lite-server",
        "start:cdn": "node server.js",
        "start:client": "ng serve --project=homepage-client",
        "start:homepage-client": "ng serve --project=homepage-client --proxy-config projects/homepage-client/proxy.conf.json",
        "test": "ng test",
        "test:watch": "ng test --watch",
        "test:coverage": "ng test --coverage",
        "test:a11y": "npm-run-all -p -r start setup-a11y",
        "setup-a11y": "wait-on http://localhost:4201 && npm run a11y",
        "prea11y": "rm -rf ./dist/a11y-reports && mkdir dist/a11y-reports",
        "a11y": "pa11y-ci --json -c .pa11yci.js > ./dist/pa11y-ci-results.json && npm run a11y-reports",
        "a11y-reports": "pa11y-ci-reporter-html -s ./dist/pa11y-ci-results.json -d ./dist/a11y-reports --include-zero-issues"
    },
    "keywords": [
        "aacom",
        "homepage",
        "angular",
        "web-product",
        "web-components",
        "custom-elements",
        "angular-elements",
        "micro-frontends"
    ],
    "author": "Steven Leija <steven.leija@aa.com> (https://ghe.aa.com/00793770)",
    "license": "MIT",
